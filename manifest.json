{
  "name": "Remember last read",
  "description": "remember last read",
  "version": "1.1.0",
  "manifest_version": 3,
  "icons": {
    "32": "images/32.png",
    "64": "images/64.png",
    "128": "images/128.png"
  },
  "permissions": ["storage", "sidePanel"],
  "action": {
    "default_title": "Click to overview TOC and view the last you read",
    "default_popup": "popup/index.html"
  },
  "side_panel": {
    "default_path": "popup/index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content_script.js"]
    }
  ]
}
